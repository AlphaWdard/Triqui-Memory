<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#77DD77"
    tools:context=".memory">

    <ImageButton
        android:id="@+id/btn8"
        android:layout_width="85dp"
        android:layout_height="86dp"
        android:layout_marginStart="31dp"
        android:layout_marginTop="46dp"
        android:background="#00FFFFFF"
        android:onClick="ver"
        android:src="@drawable/apple"
        app:layout_constraintStart_toEndOf="@+id/btn7"
        app:layout_constraintTop_toBottomOf="@+id/btn5"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageButton
        android:id="@+id/btn9"
        android:layout_width="85dp"
        android:layout_height="86dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="47dp"
        android:background="#00FFFFFF"
        android:onClick="ver"
        android:src="@drawable/strawberry__2_"
        app:layout_constraintStart_toEndOf="@+id/btn8"
        app:layout_constraintTop_toBottomOf="@+id/btn6"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageButton
        android:id="@+id/btn7"
        android:layout_width="85dp"
        android:layout_height="86dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="47dp"
        android:background="#00FFFFFF"
        android:onClick="ver"
        android:src="@drawable/banana"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn4"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageButton
        android:id="@+id/btn5"
        android:layout_width="85dp"
        android:layout_height="86dp"
        android:layout_marginStart="31dp"
        android:layout_marginTop="33dp"
        android:background="#00FFFFFF"
        android:onClick="ver"
        android:src="@drawable/plum"
        app:layout_constraintStart_toEndOf="@+id/btn4"
        app:layout_constraintTop_toBottomOf="@+id/btn2"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageButton
        android:id="@+id/btn6"
        android:layout_width="85dp"
        android:layout_height="86dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="33dp"
        android:background="#00FFFFFF"
        android:onClick="ver"
        android:src="@drawable/apple__1_"
        app:layout_constraintStart_toEndOf="@+id/btn5"
        app:layout_constraintTop_toBottomOf="@+id/btn3"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageButton
        android:id="@+id/btn4"
        android:layout_width="85dp"
        android:layout_height="86dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="33dp"
        android:background="#00FFFFFF"
        android:onClick="ver"
        android:src="@drawable/cherry"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn1"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageButton
        android:id="@+id/btn2"
        android:layout_width="85dp"
        android:layout_height="86dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="48dp"
        android:background="#00673AB7"
        android:onClick="ver"
        android:src="@drawable/raspberry"
        app:layout_constraintStart_toEndOf="@+id/btn1"
        app:layout_constraintTop_toBottomOf="@+id/txt"
        tools:src="@tools:sample/backgrounds/scenic"
        tools:srcCompat="@android:drawable/checkbox_on_background" />

    <ImageButton
        android:id="@+id/btn3"
        android:layout_width="85dp"
        android:layout_height="86dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="140dp"
        android:background="#00FFFFFF"
        android:onClick="ver"
        android:src="@drawable/pineapple"
        app:layout_constraintStart_toEndOf="@+id/btn2"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageButton
        android:id="@+id/btn1"
        android:layout_width="85dp"
        android:layout_height="86dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="141dp"
        android:background="#00FFFFFF"
        android:onClick="ver"
        android:src="@drawable/blueberry"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/txt"
        android:layout_width="177dp"
        android:layout_height="42dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="48dp"
        android:fontFamily="@font/acme"
        android:text="Memory"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textSize="34sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/Start"
        android:layout_width="120dp"
        android:layout_height="119dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="68dp"
        android:background="#00FFFFFF"
        android:foreground="@drawable/start"
        android:onClick="Start"
        android:text="Inicio"
        android:textColorHighlight="#00FFFFFF"
        android:textColorHint="#00FFFFFF"
        android:textColorLink="#00FFFFFF"
        app:iconTint="#00FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn8" />

    <TextView
        android:id="@+id/player"
        style="@style/Widget.AppCompat.TextView.SpinnerItem"
        android:layout_width="167dp"
        android:layout_height="105dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="28dp"
        android:fontFamily="@font/acme"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/txt"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtp1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="100dp"
        android:layout_marginTop="64dp"
        android:fontFamily="@font/acme"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/Start"
        app:layout_constraintTop_toBottomOf="@+id/btn9" />

    <TextView
        android:id="@+id/txtp2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="102dp"
        android:layout_marginTop="39dp"
        android:fontFamily="@font/acme"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/Start"
        app:layout_constraintTop_toBottomOf="@+id/txtp1" />

</androidx.constraintlayout.widget.ConstraintLayout>